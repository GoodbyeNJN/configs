{
    "name": "eslint-config-goodbyenjn",
    "version": "1.2.1",
    "description": "GoodbyeNJN eslint config",
    "keywords": [
        "eslint",
        "eslintrc",
        "eslintconfig",
        "config",
        "javascript",
        "styleguide",
        "react",
        "typescript"
    ],
    "homepage": "https://github.com/GoodbyeNJN/eslint-config-goodbyenjn",
    "bugs": {
        "url": "https://github.com/GoodbyeNJN/eslint-config-goodbyenjn/issues"
    },
    "license": "MIT",
    "author": "GoodbyeNJN <cc@fuckwall.cc>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GoodbyeNJN/eslint-config-goodbyenjn.git"
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./prettier": {
            "types": "./dist/prettier.d.ts",
            "import": "./dist/prettier.mjs",
            "require": "./dist/prettier.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "postbuild": "scripts/postbuild.ts"
    },
    "dependencies": {
        "@babel/core": "^7.23.5",
        "@babel/eslint-parser": "^7.23.3",
        "@babel/preset-react": "^7.23.3",
        "@typescript-eslint/eslint-plugin": "^6.13.2",
        "@typescript-eslint/parser": "^6.13.2",
        "eslint-config-alloy": "^5.1.2",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-solid": "^0.13.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.4",
        "tsx": "^4.6.2",
        "unbuild": "^2.0.0",
        "zx": "^7.2.3"
    },
    "peerDependencies": {
        "eslint": "^8.0.0",
        "prettier": "^3.0.0",
        "typescript": "^5.0.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "engines": {
        "node": ">=16"
    },
    "np": {
        "yolo": true,
        "contents": "dist"
    }
}
